// CHECK:        [sequence
// CHECK:          <while Preheader: [[IPHDR:bb[0-9]+]], Header: [[IHDR:bb[0-9]+]], exit: [[IEXIT:bb[0-9]+]]
// CHECK:            [sequence
// CHECK:              {condition Header: {{bb[0-9]+}}
// CHECK:                {condition Header: {{bb[0-9]+}}
// CHECK:                  block {{bb[0-9]+}}
// CHECK:                  block {{bb[0-9]+}}
// CHECK:                block {{bb[0-9]+}}
// CHECK:              block {{bb[0-9]+}}]>
